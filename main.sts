module ia32/elf/begin.sts

defarr bigbad 65536 ; max size of input is 64KB

set_entry ; reversed echo
    0 ; bigbad index
    do sys_read 0 = until
        over bigbad + !
        1 +
    od ; bigbad length will be on top of stack

    do
        1 - dup bigbad + @ sys_write
    dup 0 = untilod drop
sys_exit

module ia32/elf/end.sts
